    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Export JSON",
            "type": "python",
            "request": "launch",
            "program": "./crunch_uml/cli.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-it", "eaxmi",
                "-if", "./test/data/RelationTest.xml",
                "-ot", "json",
                "-of", "./test/output/RelationTest.json",
                "-db_create"
            ]
        },
        {
            "name": "Import Onderwijs Export MD",
            "type": "python",
            "request": "launch",
            "program": "./crunch_uml/cli.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-it", "eaxmi",
                "-if", "./test/data/GGM_Onderwijs_XMI.2.1.xml",
                "-ot", "ggm_md",
                "-of", "./test/output/GGM.md",
                "-db_create",
                "--output_package_ids", "EAPK_CD9BF007_85C6_4af9_B3F4_2CAB5BF26B5E"
            ]
        },
        {
            "name": "Import GGM",
            "type": "python",
            "request": "launch",
            "program": "./crunch_uml/cli.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-it", "eaxmi",
                "-if", "./test/data/Gemeentelijk Gegevensmodel.xml",
                "-db_create",
                "--skip_xmi_relations"
            ]
        },
        {
            "name": "Import GGM Monumenten",
            "type": "python",
            "request": "launch",
            "program": "./crunch_uml/cli.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-it", "eaxmi",
                "-if", "./test/data/GGM_Monumenten_EA2.1.xml"
            ]
        },
        {
            "name": "Export Only GGM",
            "type": "python",
            "request": "launch",
            "program": "./crunch_uml/cli.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-ot", "ggm_md",
                "-of", "./test/output/GGM.md",
                "--output_package_ids", "EAPK_CD9BF007_85C6_4af9_B3F4_2CAB5BF26B5E, EAPK_CD9BF007_85C6_4af9_B3F4_2CAB5BF26B5E"
            ]
        },
        {
            "name": "Import Only Test Alle relaties",
            "type": "python",
            "request": "launch",
            "program": "./crunch_uml/cli.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["-if", "./test/data/AlleRelatieSitiuaties.xml", "-it", "xmi", "-db_create"]
        }
    ]
